{
  "name": "di-wise-neo",
  "packageManager": "yarn@4.6.0",
  "version": "0.2.8",
  "description": "Lightweight, typesafe, and flexible dependency injection library for JavaScript and TypeScript",
  "files": [
    "dist",
    "src",
    "!src/__tests__"
  ],
  "main": "./dist/cjs/index.js",
  "module": "./dist/es/index.mjs",
  "types": "./dist/cjs/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/es/index.d.mts",
        "default": "./dist/es/index.mjs"
      },
      "require": {
        "types": "./dist/cjs/index.d.ts",
        "default": "./dist/cjs/index.js"
      }
    }
  },
  "sideEffects": false,
  "scripts": {
    "build": "tsc --noEmit && bunchee --sourcemap",
    "test": "vitest --coverage \"$@\"",
    "lint": "eslint . --fix --cache",
    "lint:ci": "eslint . --max-warnings 0 --cache --cache-location ./node_modules/.cache/eslint/.eslintcache",
    "format": "prettier \"**/*.{json,md,yml}\" --write --log-level warn --cache",
    "format:check": "prettier \"**/*.{json,md,yml}\" --check --cache",
    "typedoc": "typedoc src/index.ts --includeVersion"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lppedd/di-wise-neo.git"
  },
  "keywords": [
    "di",
    "ioc",
    "dependency injection",
    "inversion of control",
    "dependency",
    "injection",
    "injector",
    "container",
    "typescript",
    "experimental",
    "decorator",
    "decorators"
  ],
  "author": "Edoardo Luppi <lp.edoardo@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/lppedd/di-wise-neo/issues"
  },
  "homepage": "https://github.com/lppedd/di-wise-neo",
  "devDependencies": {
    "@eslint/js": "^9.20.0",
    "@stylistic/eslint-plugin": "^3.1.0",
    "@types/eslint__js": "^8.42.3",
    "@types/node": "^22.13.1",
    "@vitest/coverage-v8": "^3.0.5",
    "@vitest/ui": "^3.0.5",
    "bunchee": "^6.3.4",
    "eslint": "~9.19.0",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "globals": "^15.14.0",
    "prettier": "~3.4.2",
    "typedoc": "^0.27.7",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.24.0",
    "vitest": "^3.0.5"
  }
}
